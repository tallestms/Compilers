algoritmo declaracao_de_variaveis;

variáveis
	a : inteiro;
	b : inteiro;
	c : real;
	d : real;
	e : real;
	f : real;
	char : caractere;
	lit : literal;
	z : lógico;
	mat : matriz[2] de inteiros; 
fim-variáveis

início
	a := 10;
	b := -10;
	c := 10.10;
	d := -10.10;
	e := 10.10e+10;
	f := -10.10e+10;
	char := 'c';
	lit := "isso daqui é um texto";
	z := verdadeiro;
	mat := [1,2];
fim

