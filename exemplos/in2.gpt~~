algoritmo idade2;

variáveis
	idade : inteiro;
	nome : literal;
	x,fat : inteiro;
	res : inteiro;
	a,b : real;
	z : lógico;
fim-variáveis

início

	x := 10;
	fat := 10;
	res := 10;
	a:=10.10;
	b:=10.20;
	//imprima("Digite seu nome:");
	//imprima(nome, ", digite sua idade:");
	idade := 10;
	se (idade >= 18 e idade < 10 e a>10.10 e verdadeiro) então
	  idade := 10.10;
	fim-se
			res := res + x + 10.10 + 10.10;
			z := x>fat e res > fat;

	enquanto (x <> 10) faça
		res := res + x + 10.10 + 10.10;
		x := x - 1;
	fim-enquanto

	x := 10;
	res := 20;
	
	enquanto (x <> 0) faça
		res := res*x+10.10;
	fim-enquanto
	
	faça 
		res := res*x;
		x := x - 1;
	enquanto (x > 1); 	
	
	para (x de fat até 10 passo 10) faça
			faça 
			    res := res*x+10.10;
			    a := a + b+10.10;
			enquanto (x > 1); 
	fim-para
		
	
	
	// Exemplo de uso de SWITCH
	seleciona (x)
		caso (1):
			imprima("O número impresso foi 1.\n");
			parar;
		caso (2):
			imprima("O número impresso foi 2.\n");
			parar;
		caso (3):
			imprima("O número impresso foi 3.\n");
		caso (4):
			imprima("O número impresso foi 4.\n");
			parar;
		padrão:
			parar;
	fim-seleciona
	
fim
