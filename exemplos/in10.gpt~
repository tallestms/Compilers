função fibonacci(a:inteiro,b:inteiro,c:inteiro)
	d:inteiro;
	i:inteiro;
início
	//imprimaln("parametros:");
	//imrpima(a);
	//imprima(" ");
	//imrpima(b);
	//imprima(" ");
	//imrpimaln(c);
	//d:=a+b;
	i:=c-1;
	imprima(d);
	imprima(", ");
	 se (c>0) então
	 	fibonacci(b, d, i);
	 fim-se
fim



variáveis
	a : inteiro;
	b : inteiro;
fim-variáveis

início
	fibonacci(1,1,30);
fim	
